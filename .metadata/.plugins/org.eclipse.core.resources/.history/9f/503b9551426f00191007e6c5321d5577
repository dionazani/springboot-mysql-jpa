package org.abccourse.service.master;

import java.util.List;

import org.abccourse.model.CourseModel;
import org.abccourse.model.StudentCourseModel;

public interface CourseService {

	CourseModel getCourseByCode(String code);
	List<CourseModel> getCourseByName(String name);
	List<StudentCourseModel> getStudentCourseByStudentName(String studentName);
}
